<div class="col-md-12">
  <div class="col-md-6">
    <form action="/projects" method="post">
    <button class="btn btn-primary MEGA" type="submit" name="button">Projects</button>
    </form>
  </div>
  <div class="col-md-6">
    <form action="/volunteers" method="post">
    <button class="btn btn-success MEGA" type="submit" name="button">Volunteers</button>
    </form>
  </div>
  <br>
  <hr>
  <br>
  <div id="projects_list">
    <h2>All Projects:</h2>
    <% Project.all.to_a.each do |project| %>
      <a href="/project/<%= project['id'] %>"><%= project['name'] %></a><br>
    <% end %>
    <br>
  <a href="/" class="btn btn-info">Home</a>
</div>
